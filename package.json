  "devDependencies": {
    "@0xproject/typescript-typings": "^0.3.2",
    "@marketprotocol/types": "^0.1.5",
    "@types/aws-lambda": "^8.10.6",
    "@types/bn.js": "^4.11.0",
    "@types/lodash.values": "^4.3.3",
    "@types/node": "^10.3.3",
